class PrintNumbers extends Thread {
    int start, end;

    public PrintNumbers(int s, int e) {
        start = s;
        end = e;
    }

    public void run() {
        for (int i = start; i <= end; i++) {
            System.out.println(i);
        }
    }

    public static void main(String[] args) {
        new PrintNumbers(1, 5).start();
        new PrintNumbers(6, 10).start();
    }
}
