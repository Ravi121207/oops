class Employee {
    String name;
    int id;
    double salary;

    Employee(String n, int i, double s) {
        name = n; id = i; salary = s;
    }

    void display() {
        System.out.println("ID: " + id + ", Name: " + name + ", Salary: " + salary);
    }

    public static void main(String[] args) {
        Employee e = new Employee("John", 101, 50000);
        e.display();
    }
}
